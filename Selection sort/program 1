class Solution
{

	
	void selectionSort(int arr[], int n)
    	{   int min;
        	for(int i=0;i<n;i++){
    	        min=i;
    	        for(int j=0;j<n;j++){
    	            if(arr[min]<arr[j]){
    	                min=j;
    	            }
    	            int temp=arr[min];
    	            arr[min]=arr[i];
    	            arr[i]=temp;
    	        }
    	    }
    	    
	    }
}	
