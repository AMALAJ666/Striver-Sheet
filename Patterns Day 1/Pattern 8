public class Solution {
    public static void nStarTriangle(int n) {
      for(int i=n;i>=1;i--){
             for(int j=n-i;j>=1;j--){
                System.out.print(" ");

            }
            for(int j=2*i-1;j>=1;j--){
                System.out.print("*");

            }
           
            System.out.println();
        }
    }
}
